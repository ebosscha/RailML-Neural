<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vw="clr-namespace:RailML___WPF.RailMLViewer.Views"
                    xmlns:vm="clr-namespace:RailML___WPF.RailMLViewer.ViewModels">
    <DataTemplate DataType="{x:Type vm:TracksViewModel}">
        <vw:TracksView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OcpViewModel}">
        <vw:OcpView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:NetworkDrawingViewModel}">
        <vw:NetworkDrawingView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SelectedPropertiesViewModel}">
        <vw:SelectedProperties/>
    </DataTemplate>
    



</ResourceDictionary>