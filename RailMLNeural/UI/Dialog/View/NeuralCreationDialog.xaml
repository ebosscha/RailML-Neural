<Window x:Class="RailMLNeural.UI.Dialog.View.NeuralCreationDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NeuralCreationDialog" Height="200" Width="300">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock Text="Which Type of Neural Network to Create?"
                       Margin="20,20,20,20"/>
            <Button Content="Create Graph Recurrent Network"
                    Height="20"
                    Click="GraphRecurrent_Click"/>
            <Button Content="Create Recursive Network"
                    Height="20"
                    Click="Recursive_Click"/>
            <Button Content="Create NEAT Network"
                    Height="20"
                    Click="NEAT_Click"/>
            <Button Content="Create LSTM Network"
                    Height="20"
                    Click="LSTM_Click"/>
            <Button Content="Create Normal Network"
                    Height="20"
                    Click="Normal_Click"/>
            <Button Content="Cancel"
                    Height="20"
                    Click="Cancel_Click"/>
            
            
        </StackPanel>
        
    </Grid>
</Window>
